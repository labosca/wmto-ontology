    <!-- https://schema.org/employmentType -->


    <owl:ObjectProperty rdf:about="https://schema.org/employmentType">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/org#Post"/>
        <rdfs:range rdf:resource="https://w3id.org/wmto#EmploymentTypeEnum"/>
        <rdfs:comment xml:lang="en">Type of employment (e.g. full-time, part-time, contract, temporary, seasonal, internship).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/employmentType"/>
