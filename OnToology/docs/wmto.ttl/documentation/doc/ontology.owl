    </owl:ObjectProperty>
    


    <!-- https://schema.org/employmentUnit -->


    <owl:ObjectProperty rdf:about="https://schema.org/employmentUnit">
        <rdfs:range rdf:resource="http://www.w3.org/ns/org#OrganizationalUnit"/>
        <rdfs:comment xml:lang="en">Indicates the department, unit and/or facility where the employee reports and/or in which the job is to be performed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://pending.schema.org/employmentUnit"/>
        <rdfs:label xml:lang="en">employmentUnit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/wmto#assetType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/wmto#assetType">
        <rdfs:domain rdf:resource="https://w3id.org/rec#Asset"/>
        <rdfs:comment>To link an Asset (e.g. ICT Asset, Furniture) to its TypeEnum individual, depending on those defined by the use case.</rdfs:comment>
        <rdfs:label>assetType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/wmto#definesAAOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/wmto#definesAAOf">
        <owl:inverseOf rdf:resource="https://w3id.org/wmto#isDefinedByAA"/>
        <rdfs:domain rdf:resource="https://w3id.org/wmto#AssetAllocation"/>
        <rdfs:range rdf:resource="https://w3id.org/rec#Asset"/>
        <rdfs:comment>To link an Asset Allocation instance to a piece of Furniture/ICTEquipment</rdfs:comment>
        <rdfs:label>definesAssetAllocationOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/wmto#definesWPAOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/wmto#definesWPAOf">
        <owl:inverseOf rdf:resource="https://w3id.org/wmto#isDefinedByWPA"/>
        <rdfs:domain rdf:resource="https://w3id.org/wmto#WPAllocation"/>
        <rdfs:range rdf:resource="https://w3id.org/rec#Office"/>
        <rdfs:comment>To link a WorkPlace Allocation instance to a Space/Office/Workplace</rdfs:comment>
        <rdfs:label>definesWorkPlaceAllocationOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/wmto#hasAA -->


    <owl:ObjectProperty rdf:about="https://w3id.org/wmto#hasAA">
        <owl:inverseOf rdf:resource="https://w3id.org/wmto#isAATo"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                    <rdf:Description rdf:about="https://w3id.org/rec#Room"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/wmto#AssetAllocation"/>
        <rdfs:comment>To link an Agent (Person, OrgUnit, Org) or a Room (e.g., Office) to an Asset Allocation instance</rdfs:comment>
        <rdfs:label>hasAssetAllocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/wmto#hasWPA -->


    <owl:ObjectProperty rdf:about="https://w3id.org/wmto#hasWPA">
        <owl:inverseOf rdf:resource="https://w3id.org/wmto#isWPATo"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/wmto#WPAllocation"/>
        <rdfs:comment>To link a Person/employee to a WorkPlaceAllocation instance</rdfs:comment>
        <rdfs:label>hasWorkPlaceAllocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/wmto#isAATo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/wmto#isAATo">
        <rdfs:domain rdf:resource="https://w3id.org/wmto#AssetAllocation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                    <rdf:Description rdf:about="https://w3id.org/rec#Room"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>To link an AssetAllocation instance to an Agent (Person or Organization/OrgUnit) or a Room (e.g., Office)</rdfs:comment>
        <rdfs:label>isAssetAllocationTo</rdfs:label>
    </owl:ObjectProperty>
    


