###  https://schema.org/employmentType
sdo:employmentType rdf:type owl:ObjectProperty ;
                   rdfs:domain org:Post ;
                   rdfs:range wmto:EmploymentTypeEnum ;
                   rdfs:comment "Type of employment (e.g. full-time, part-time, contract, temporary, seasonal, internship)."@en ;
                   rdfs:isDefinedBy sdo:employmentType ;
                   rdfs:label "employmentType"@en .


###  https://schema.org/employmentUnit
sdo:employmentUnit rdf:type owl:ObjectProperty ;
                   rdfs:range org:OrganizationalUnit ;
                   rdfs:comment "Indicates the department, unit and/or facility where the employee reports and/or in which the job is to be performed."@en ;
                   rdfs:isDefinedBy <https://pending.schema.org/employmentUnit> ;
                   rdfs:label "employmentUnit"@en .


###  https://w3id.org/wmto#assetType
wmto:assetType rdf:type owl:ObjectProperty ;
               rdfs:domain rec:Asset ;
               rdfs:comment "To link an Asset (e.g. ICT Asset, Furniture) to its TypeEnum individual, depending on those defined by the use case." ;
               rdfs:label "assetType" .


###  https://w3id.org/wmto#definesAAOf
wmto:definesAAOf rdf:type owl:ObjectProperty ;
                 owl:inverseOf wmto:isDefinedByAA ;
