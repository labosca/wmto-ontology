                  rdfs:label "definesWorkPlaceAllocationOf" .


###  https://w3id.org/wmto#hasAA
wmto:hasAA rdf:type owl:ObjectProperty ;
           owl:inverseOf wmto:isAATo ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( foaf:Agent
                                       rec:Room
                                     )
                       ] ;
           rdfs:range wmto:AssetAllocation ;
           rdfs:comment "To link an Agent (Person, OrgUnit, Org) or a Room (e.g., Office) to an Asset Allocation instance" ;
           rdfs:label "hasAssetAllocation" .


